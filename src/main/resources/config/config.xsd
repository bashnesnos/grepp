<?xml version="1.0" encoding="UTF-8"?>
<schema targetNamespace="http://www.smltools.org/config"
	elementFormDefault="qualified" xmlns="http://www.w3.org/2001/XMLSchema"
	xmlns:tns="http://www.smltools.org/config">

	<complexType name="root">
		<sequence>
			<element ref="tns:defaults" maxOccurs="1" minOccurs="0"></element>
			<element ref="tns:postProcessSeparators" maxOccurs="1" minOccurs="0"></element>
			<element ref="tns:logDateFormats" maxOccurs="1" minOccurs="0"></element>
			<element ref="tns:config" maxOccurs="unbounded" minOccurs="0"></element>
			<element ref="tns:processThreads" maxOccurs="1" minOccurs="0"></element>
			<element ref="tns:postProcessColumns" maxOccurs="1" minOccurs="0"></element>
			<element ref="tns:filterAliases" maxOccurs="1" minOccurs="0"></element>
		</sequence>
	</complexType>


	<complexType name="defaults">
		<sequence>
			<element ref="tns:spoolFileExtension" maxOccurs="1" minOccurs="0"></element>
            <element ref="tns:resultsDir" maxOccurs="1" minOccurs="0"></element>
            <element ref="tns:postProcessSeparator" maxOccurs="1" minOccurs="0"></element>
		</sequence>
	</complexType>

	<complexType name="postProcessSeparators">
		<sequence>
			<element ref="tns:separator" maxOccurs="unbounded" minOccurs="1"></element>
		</sequence>
	</complexType>


	<complexType name="separator">
		<sequence>
			<element ref="tns:spoolFileExtension" maxOccurs="1" minOccurs="1"></element>
			<element ref="tns:value" maxOccurs="1" minOccurs="1"></element>
		</sequence>
		<attribute name="id" type="string"></attribute>				
	</complexType>

	<complexType name="logDateFormats">
		<sequence>
			<element ref="tns:dateFormat" maxOccurs="unbounded" minOccurs="1"></element>
		</sequence>
	</complexType>

	<complexType name="dateFormat">
		<sequence>
			<element ref="tns:value" maxOccurs="1" minOccurs="1"></element>
			<element ref="tns:regex" maxOccurs="1" minOccurs="1"></element>
		</sequence>
		<attribute name="id" type="string"></attribute>				
	</complexType>

	<complexType name="pattern" mixed="true">
		<simpleContent>
			<extension base="string">
				<attribute name="tags" type="string"></attribute>
			</extension>
		</simpleContent>
	</complexType>

	<element name="spoolFileExtension" type="string"></element>

	<element name="postProcessSeparator" type="string"></element>

	<element name="value" type="string"></element>

	<element name="regex" type="string"></element>

	<element name="separator" type="tns:separator"></element>

    <element name="resultsDir" type="string"></element>

	<element name="pattern" type="tns:pattern"></element>

	<complexType name="config">
		<sequence>
			<element ref="tns:logDateFormat" maxOccurs="1" minOccurs="0"></element>
			<element ref="tns:starter" maxOccurs="1" minOccurs="0"></element>
			<element ref="tns:logThreshold" maxOccurs="1" minOccurs="0"></element>
			<element ref="tns:pattern" maxOccurs="1" minOccurs="0"></element>
		</sequence>
		<attribute name="id" type="string"></attribute>
	</complexType>

	<element name="logDateFormat" type="string"></element>

	<element name="starter" type="string"></element>

	<element name="date" type="string"></element>

	<element name="logThreshold" type="string"></element>

	<element name="config" type="tns:config"></element>

	<complexType name="processThreads">
		<sequence>
			<element ref="tns:extractors" maxOccurs="1" minOccurs="1"></element>
			<element ref="tns:skipends" maxOccurs="1" minOccurs="0"></element>
			<element ref="tns:ends" maxOccurs="1" minOccurs="1"></element>
		</sequence>
	</complexType>



	<complexType name="thread_patterns">
		<sequence>
			<element ref="tns:pattern" maxOccurs="unbounded" minOccurs="1"></element>
		</sequence>
	</complexType>

	<element name="extractors" type="tns:thread_patterns"></element>

	<element name="skipends" type="tns:thread_patterns"></element>

	<element name="ends" type="tns:thread_patterns"></element>

	<element name="processThreads" type="tns:processThreads"></element>

	<complexType name="postProcessColumns">
		<sequence>
			<element ref="tns:splitter" maxOccurs="unbounded" minOccurs="1"></element>
		</sequence>
	</complexType>

	<complexType name="splitter">
		<simpleContent>
			<extension base="tns:pattern">
				<attribute name="col_name" type="string"></attribute>
				<attribute name="type" type="string"></attribute>
			</extension>
		</simpleContent>
	</complexType>

	<element name="splitter" type="tns:splitter"></element>

	<element name="postProcessColumns" type="tns:postProcessColumns"></element>

	<complexType name="filterAliases">
		<sequence>
			<element ref="tns:filter" maxOccurs="unbounded" minOccurs="1"></element>
		</sequence>
	</complexType>


	<element name="filter" type="tns:pattern"></element>

	<element name="filterAliases" type="tns:filterAliases"></element>

	<element name="defaults" type="tns:defaults"></element>

	<element name="postProcessSeparators" type="tns:postProcessSeparators"></element>

	<element name="root" type="tns:root"></element>

